    <validationRules>
        <fullName>wkcc__LimitNameLength</fullName>
        <active>true</active>
        <description>Limit location name to 40 characters to ensure maximum compatibility with Work.com add-on packages.</description>
        <errorConditionFormula>( ISNEW() || ISCHANGED(Name) ) &amp;&amp; LEN(Name) &gt; 40</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Location name is limited to 40 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkfsl__Location_name_maximum_40</fullName>
        <active>true</active>
        <description>Location name must be lower or equal to 40 characters.</description>
        <errorConditionFormula>LEN(Name) &gt; 40</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Location name must be lower or equal to 40 characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkfsl__Timezone_Required_Field</fullName>
        <active>true</active>
        <description>Timezone field must be populated for shift management data creation</description>
        <errorConditionFormula>ISPICKVAL( TimeZone , &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>TimeZone</errorDisplayField>
        <errorMessage>Location must have timezone field populated</errorMessage>
    </validationRules>
