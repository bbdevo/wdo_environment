    <validationRules>
        <fullName>wkcc__EndDateToBeOnOrAfterStartDate</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISNULL( wkcc__EndDate__c )),
DATEVALUE(wkcc__StartDate__c) &gt; wkcc__EndDate__c)</errorConditionFormula>
        <errorDisplayField>wkcc__EndDate__c</errorDisplayField>
        <errorMessage>End Date needs to be on or after Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkcc__RecipientTypeNotChangeable</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(wkcc__RecipientType__c)</errorConditionFormula>
        <errorDisplayField>wkcc__RecipientType__c</errorDisplayField>
        <errorMessage>You can&apos;t change the recipient type after a schedule is created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkcc__StartDateNeedsToBeFuture</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    OR(
        ISNEW(),
        ISCHANGED(wkcc__StartDate__c) 
    ),
        wkcc__StartDate__c &lt; Now() - (1/1440)
)</errorConditionFormula>
        <errorDisplayField>wkcc__StartDate__c</errorDisplayField>
        <errorMessage>Give the schedule a start date that&apos;s in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkcc__UpdateStartDateToFuture</fullName>
        <active>true</active>
        <description>When the user reactivate or update the survey schedule, update the start date to a future date if the date is a previous date time.</description>
        <errorConditionFormula>AND (!ISCHANGED(wkcc__StartDate__c),
    wkcc__StartDate__c &lt;= NOW(),
    OR (AND ( !PRIORVALUE( wkcc__Active__c ), wkcc__Active__c),
        ISCHANGED( wkcc__ScheduleFrequency__c )
    )
)</errorConditionFormula>
        <errorDisplayField>wkcc__StartDate__c</errorDisplayField>
        <errorMessage>Give the schedule a start date that&apos;s in the future.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
