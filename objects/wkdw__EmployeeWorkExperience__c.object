    <validationRules>
        <fullName>wkdw__CompletionDate</fullName>
        <active>true</active>
        <description>Validates that the end date comes after the start date.</description>
        <errorConditionFormula>AND(NOT(wkdw__Ongoing__c ),wkdw__StartDate__c  &gt; wkdw__CompletionDate__c)</errorConditionFormula>
        <errorDisplayField>wkdw__CompletionDate__c</errorDisplayField>
        <errorMessage>Enter an end date that occurs after the start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkdw__URLs</fullName>
        <active>true</active>
        <description>Validates the format of the URL.</description>
        <errorConditionFormula>IF(LEN(TRIM(wkdw__URLs__c)) &gt; 0, NOT(REGEX( wkdw__URLs__c , &quot;^((([a-zA-Z]+:)?\\/\\/.*)\\r?\\n?)*$&quot;)), false)</errorConditionFormula>
        <errorDisplayField>wkdw__URLs__c</errorDisplayField>
        <errorMessage>Enter a URL that starts with a valid protocol such as https:// or file:///.</errorMessage>
    </validationRules>
