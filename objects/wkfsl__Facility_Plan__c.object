    <validationRules>
        <fullName>wkfsl__Completion_Date_Before_Plan_Start</fullName>
        <active>false</active>
        <description>Validates that the availability request due date is before the facility plan’s start date.</description>
        <errorConditionFormula>wkfsl__Start_Date__c  &lt;=  wkfsl__Completion_Date__c</errorConditionFormula>
        <errorDisplayField>wkfsl__Completion_Date__c</errorDisplayField>
        <errorMessage>Enter a due date that’s before the plan start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkfsl__Completion_Date_Not_In_Past</fullName>
        <active>false</active>
        <description>Enter a due date later than today.</description>
        <errorConditionFormula>TODAY() &gt; wkfsl__Completion_Date__c</errorConditionFormula>
        <errorDisplayField>wkfsl__Completion_Date__c</errorDisplayField>
        <errorMessage>Completion date can&apos;t be on a past date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkfsl__Max_allowed_employees_must_be_positive</fullName>
        <active>true</active>
        <errorConditionFormula>wkfsl__Max_Number_Of_Allowed_Employees__c &lt; 0</errorConditionFormula>
        <errorMessage>Max number of allowed employees of staggering plan must be a positive number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkfsl__Occupancy_Per_Interval_must_be_positive</fullName>
        <active>true</active>
        <description>Occupancy Per Interval must be positive or blank</description>
        <errorConditionFormula>NOT(ISBLANK(wkfsl__Max_Number_Of_Allowed_Employees__c))  &amp;&amp; wkfsl__Max_Number_Of_Allowed_Employees__c  = 0</errorConditionFormula>
        <errorMessage>The Occupancy Per Interval must be above 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkfsl__Percentage_Allocation</fullName>
        <active>true</active>
        <description>Percentage_Allocation__c must be between 0 to 100</description>
        <errorConditionFormula>OR(
   wkfsl__Percentage_Allocation__c &gt; 1.0,
   wkfsl__Percentage_Allocation__c &lt; 0.0
)</errorConditionFormula>
        <errorMessage>Percentage Allocation must be between 0 to 100.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkfsl__Stagger_Time_Interval</fullName>
        <active>true</active>
        <description>Stagger Time Interval must be positive</description>
        <errorConditionFormula>wkfsl__Time_Interval__c  &lt; 0</errorConditionFormula>
        <errorMessage>Time interval of staggering must be a positive number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkfsl__Stagger_plan_completeness</fullName>
        <active>true</active>
        <errorConditionFormula>((ISNULL(wkfsl__Max_Number_Of_Allowed_Employees__c) || ISNULL(wkfsl__Stagger_Start_Time__c) || ISNULL(wkfsl__Stagger_End_Time__c) || ISNULL(wkfsl__Time_Interval__c)))  &amp;&amp; (NOT(ISNULL(wkfsl__Max_Number_Of_Allowed_Employees__c)) || NOT(ISNULL(wkfsl__Stagger_Start_Time__c)) || NOT(ISNULL(wkfsl__Stagger_End_Time__c)) || NOT(ISNULL(wkfsl__Time_Interval__c)))</errorConditionFormula>
        <errorMessage>To stagger arrivals, fill out all the fields. Or, delete the Staggered Arrivals values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkfsl__Stagger_start_after_end</fullName>
        <active>true</active>
        <description>Staggering start time can&apos;t be after end time</description>
        <errorConditionFormula>wkfsl__Stagger_End_Time__c  &lt;=  wkfsl__Stagger_Start_Time__c</errorConditionFormula>
        <errorMessage>Staggering start time can&apos;t be after end time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkfsl__Time_Interval_must_be_positive</fullName>
        <active>true</active>
        <description>Time Interval must be positive or blank</description>
        <errorConditionFormula>NOT(ISBLANK(wkfsl__Time_Interval__c))  &amp;&amp; wkfsl__Time_Interval__c  = 0</errorConditionFormula>
        <errorMessage>Time Interval must be above 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>wkfsl__Work_type_should_not_auto_create</fullName>
        <active>true</active>
        <description>Linked work type should not include auto creation of services as this done by executing the facility plan. Otherwise we will end up in duplicated data.</description>
        <errorConditionFormula>NOT(ISBLANK(wkfsl__Work_Type__c)) &amp;&amp;  wkfsl__Work_Type__r.ShouldAutoCreateSvcAppt  = true</errorConditionFormula>
        <errorDisplayField>wkfsl__Work_Type__c</errorDisplayField>
        <errorMessage>Choose another work type. This work type creates service appointments automatically, which causes duplicate shifts.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
