            <value>Case.wkitsm__Change,Case.wkitsm__Incident,Case.wkitsm__Problem,Case.wkitsm__Request,Case.wkitsm__MajorIncident</value>
        </criteriaItems>
        <criteriaItems>
            <column>HSTATUS</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>notEqual</operator>
            <value>Resolved,Closed</value>
        </criteriaItems>
        <language>en_US</language>
    </filter>
